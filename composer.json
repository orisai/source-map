{
	"name": "orisai/source-map",
	"description": "Generate, validate and serialize source maps",
	"keywords": [
		"generating",
		"map",
		"orisai",
		"source",
		"validation",
		"serialization"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/source-map",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.3.0",
		"orisai/exceptions": "^1.0.0",
		"orisai/utils": "^1.0.0",
		"symfony/filesystem": "^5.4.0|^6.0.0"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.26.0",
		"orisai/coding-standard": "^3.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0"
	},
	"autoload": {
		"psr-4": {
			"Orisai\\SourceMap\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Orisai\\SourceMap\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"infection/extension-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
